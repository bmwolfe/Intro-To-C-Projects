#include <stdio.h>

int main()
{

	int salary[10], i, n;

	printf("Enter number of sales people: ");
	scanf("%d", &n);

	for(i=0; i<n; ++i){

		printf("Enter the gross sales for this person ");
		scanf("%d", &salary[i]);

		salary[i] = 200 + salary[i] * .09;

	}

	int a=0, b=0, c=0, d=0, e=0, f=0, g=0, h=0, j=0;

for(i = 0; i < n; ++i){

	if(200 <= salary[i] && salary[i] <= 299){
		++a;
	}

	else if(300 <= salary[i] && salary[i] <= 399){
		++b;
	}

	else if(400 <= salary[i] && salary[i] <= 499){
		++c;
	}

	else if(500 <= salary[i] && salary[i] <= 599){
		++d;
	}

	else if(600 <= salary[i] && salary[i] <= 699){
		++e;
	}

	else if(700 <= salary[i] && salary[i] <= 799){
		++f;
	}

	else if(800 <= salary[i] && salary[i] <= 899){
		++g;
	}

	else if(900 <= salary[i] && salary[i] <= 999){
		++h;
	}

	else {
		++j;
	}



}
printf("%d salespeople made $200-$299\n%d salespeople made $300-$399\n%d salespeople made $400-$499\n%d salespeople made $500-$599\n%d salespeople made $600-$699\n%d salespeople made $700-$799\n%d salespeople made $800-$899\n%d salespeople made $900-$999\n%d salespeople made $1000 or more\n", a, b, c, d, e, f, g, h, j);
}
