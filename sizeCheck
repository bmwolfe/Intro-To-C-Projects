#include <stdio.h>

int main()
{

	int n;

	for(int i=1; i<=100; i++)
	{

	n = i;
	printf("%d  ",i);

	while( n != 0 ){
	
	if (n >= 100){
		printf("C");
	n -= 100;
	}

	else if (n >= 90){
		printf("XC");
	n -= 90;
	}

	else if (n >= 50){
		printf("L");
	n -= 50;
	}

	else if (n >= 40){
		printf("XL");
	n -= 40;
	}

	else if (n >= 10){
		printf("X");
	n -= 10;
	}

	else if (n >= 9){
		printf("IX");
	n -= 9;
	}

	else if (n >= 5){
		printf("V");
	n -= 5;
	}

	else if (n >= 4){
		printf("IV");
	n -= 4;
	}

	else if (n >= 1){
		printf("I");
	n -= 1;
	}
}
	printf("\n");
	
	}

} 
