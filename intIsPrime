#include <stdio.h> 

int isPrime(int n);

int main ()
{

	int num;

	printf("Enter a number: ");
	scanf("%d", &num);

	printf("%d", isPrime(num));

	return 0;

}

int isPrime(int n){

	int i;
	int x;

	for(i = 2; i < n-1 ; i++) {

	if(n%i==0) {
            x = 0;
        }
} 

	if(x==1) {
		printf("%d is prime\n", n);
	}

	else {
		printf("%d is not prime\n", n);
	}
	return 0;
}
